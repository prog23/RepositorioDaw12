package NumerosNaturales;

import java.util.Scanner;

public class NumerosNaturales {

	public static void main(String[] args) {
		Scanner in=new Scanner(System.in);
		int limite;
		int n1=0;
		int n2=2;
		int total;
		
		System.out.println("Introduce el límite que deseas");
		limite=in.nextInt();
		
		
		while (limite > 0) {
			total=n1*n2;
			
		}
		

	}

}
