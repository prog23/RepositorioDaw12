package Primos;

import java.util.Scanner;

public class Fibonacci {

	public static void main(String[] args) {

		Scanner in=new Scanner(System.in);
		int limite;
		int n1=0;
		int n2=1;
		
		System.out.println("Introduce el límite que deseas");
		limite=in.nextInt();
		
		for (int i=0;i<limite-2;i++) {
			n2=n1+n2;
			n1=n2-n1;
		}

	}

}
