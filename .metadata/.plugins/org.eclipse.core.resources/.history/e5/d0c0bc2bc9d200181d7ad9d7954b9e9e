package Ejercicio1;

import java.util.Scanner;

public class Ejercicio1 {

	public static void main(String[] args) {
		
		Scanner in=new Scanner(System.in);
		int n;
		int m;
		int lineas;
		
		System.out.println("Introduce el número de líneas que deseas");
		lineas=in.nextInt();
		
		for (n=1;n>=lineas;n++) {
			System.out.print(" "+" "+" "+"*"+" "+" "+" ");
			for (m=1;m<=lineas;m++) {
				System.out.println(" "+" "+"*"+" "+" ");
				
			}
		}

	}

}
